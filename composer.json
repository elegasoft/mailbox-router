{
    "name": "elegasoft/mailbox",
    "description": "A Boilerplate for registering and managing Mailboxes with beyondcode/laravel-mailbox",
    "keywords": [
        "elegasoft",
        "mailbox"
    ],
    "homepage": "https://github.com/elegasoft/mailbox",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jason Cook",
            "email": "jason@elegasoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "beyondcode/laravel-mailbox": "^2.1",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.0|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Elegasoft\\Mailbox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elegasoft\\Mailbox\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MailboxRouterServiceProvider"
            ],
            "aliases": {
                "Mailbox": "Elegasoft\\Mailbox\\MailboxFacade"
            }
        }
    }
}
